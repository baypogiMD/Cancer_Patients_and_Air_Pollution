import pandas as pd
import sqlite3

conn = sqlite3.connect('cancer.db')

query = """
SELECT air_pollution,
       COUNT(*) AS patients,
       SUM(CASE WHEN lung_cancer_level = 2 THEN 1 ELSE 0 END) AS high_risk
FROM patients
GROUP BY air_pollution
ORDER BY air_pollution DESC;
"""

df = pd.read_sql(query, conn)
df['high_risk_rate'] = df.high_risk / df.patients

print(df)
