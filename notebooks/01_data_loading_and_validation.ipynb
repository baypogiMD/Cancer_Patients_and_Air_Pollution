import pandas as pd
import sqlite3

# Load raw CSV
df = pd.read_csv('data/raw/cancer_air_pollution.csv')

# Basic validation
assert df.isnull().sum().sum() == 0, "Missing values detected"

# Connect to SQLite
conn = sqlite3.connect('cancer.db')

# Load into SQL
df.to_sql('patients', conn, if_exists='append', index=False)

print('Rows inserted:', len(df))
