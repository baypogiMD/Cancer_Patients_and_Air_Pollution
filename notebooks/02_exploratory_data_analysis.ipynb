import pandas as pd
import sqlite3
import seaborn as sns
import matplotlib.pyplot as plt

conn = sqlite3.connect('cancer.db')
df = pd.read_sql('SELECT * FROM patients', conn)

sns.countplot(x='lung_cancer_level', data=df)
plt.title('Cancer Level Distribution')
plt.show()

sns.boxplot(x='lung_cancer_level', y='air_pollution', data=df)
plt.title('Air Pollution vs Cancer Level')
plt.show()

plt.figure(figsize=(10,8))
sns.heatmap(df.corr(numeric_only=True), cmap='coolwarm')
plt.title('Correlation Matrix')
plt.show()
