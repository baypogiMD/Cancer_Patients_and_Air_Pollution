import pandas as pd
import sqlite3
from scipy.stats import kruskal

conn = sqlite3.connect('cancer.db')
df = pd.read_sql('SELECT * FROM patients', conn)

groups = [
    df[df.lung_cancer_level == lvl]['air_pollution']
    for lvl in df.lung_cancer_level.unique()
]

stat, p = kruskal(*groups)
print('Kruskal-Wallis p-value:', p)
